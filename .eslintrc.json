{
    "env": {
        "commonjs": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:es/no-new-in-es2015",
        "plugin:es/no-new-in-es2016",
        "plugin:es/no-new-in-es2017",
        "plugin:es/no-new-in-es2018"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "plugins": [
        "es"
    ],
    "rules": {
        "es/no-modules": "off",
        "no-shadow-restricted-names": "off",
        "no-constant-condition": "off",
        "no-cond-assign": "off",
        "no-fallthrough": "off",
        "no-redeclare": "off",
        "eqeqeq": "error",
        "quotes": [
            2,
            "double"
        ]
    },
    "globals": {
        "URL": "off",
        "Blob": "off",
        "File": "off",
        "Node": "off",
        "Symbol": "off",
        "Buffer": "off",
        "isNaN": "off",
        "isFinite": "off",
        "setTimeout": "off",
        "setInterval": "off",
        "setImmediate": "off",
        "clearTimeout": "off",
        "clearInterval": "off",
        "clearImmediate": "off",
        "decodeURIComponent": "off",
        "encodeURIComponent": "off",
        "requestAnimationFrame": "off",
        "cancelAnimationFrame": "off",
        "unescape": "off",
        "escape": "off",
        "atob": "off",
        "btoa": "off",
        "self": "off",
        "window": "off",
        "global": "off",
        "globalThis": "off",
        "localStorage": "off",
        "performance": "off",
        "navigator": "off",
        "document": "off",
        "location": "off",
        "history": "off",
        "console": "off",
        "process": "off",
        "Int8Array": "off",
        "Int16Array": "off",
        "Int32Array": "off",
        "Uint8Array": "off",
        "Uint8ClampedArray": "off",
        "Uint16Array": "off",
        "Uint32Array": "off",
        "Float32Array": "off",
        "Float64Array": "off",
        "BigInt64Array": "off",
        "BigUint64Array": "off",
        "AnimationEvent": "off",
        "BeforeUnloadEvent": "off",
        "ClipboardEvent": "off",
        "CompositionEvent": "off",
        "DeviceMotionEvent": "off",
        "DeviceOrientationEvent": "off",
        "DragEvent": "off",
        "ErrorEvent": "off",
        "FocusEvent": "off",
        "GamepadEvent": "off",
        "HashChangeEvent": "off",
        "InputEvent": "off",
        "KeyboardEvent": "off",
        "MessageEvent": "off",
        "PageTransitionEvent": "off",
        "PointerEvent": "off",
        "PopStateEvent": "off",
        "ProgressEvent": "off",
        "PromiseRejectionEvent": "off",
        "SecurityPolicyViolationEvent": "off",
        "StorageEvent": "off",
        "TouchEvent": "off",
        "TransitionEvent": "off",
        "WheelEvent": "off",
        "MouseEvent": "off",
        "UIEvent": "off",
        "HTMLAnchorElement": "off",
        "HTMLAreaElement": "off",
        "HTMLAudioElement": "off",
        "HTMLBRElement": "off",
        "HTMLBaseElement": "off",
        "HTMLBodyElement": "off",
        "HTMLButtonElement": "off",
        "HTMLCanvasElement": "off",
        "HTMLDListElement": "off",
        "HTMLDataElement": "off",
        "HTMLDataListElement": "off",
        "HTMLDetailsElement": "off",
        "HTMLDialogElement": "off",
        "HTMLDivElement": "off",
        "HTMLEmbedElement": "off",
        "HTMLFieldSetElement": "off",
        "HTMLFormElement": "off",
        "HTMLHRElement": "off",
        "HTMLHeadElement": "off",
        "HTMLHeadingElement": "off",
        "HTMLHtmlElement": "off",
        "HTMLIFrameElement": "off",
        "HTMLImageElement": "off",
        "HTMLInputElement": "off",
        "HTMLLIElement": "off",
        "HTMLLabelElement": "off",
        "HTMLLegendElement": "off",
        "HTMLLinkElement": "off",
        "HTMLMenuElement": "off",
        "HTMLMetaElement": "off",
        "HTMLMeterElement": "off",
        "HTMLModElement": "off",
        "HTMLOListElement": "off",
        "HTMLObjectElement": "off",
        "HTMLOptGroupElement": "off",
        "HTMLOptionElement": "off",
        "HTMLOutputElement": "off",
        "HTMLParagraphElement": "off",
        "HTMLParamElement": "off",
        "HTMLPictureElement": "off",
        "HTMLPortalElement": "off",
        "HTMLPreElement": "off",
        "HTMLProgressElement": "off",
        "HTMLQuoteElement": "off",
        "HTMLScriptElement": "off",
        "HTMLSelectElement": "off",
        "HTMLSlotElement": "off",
        "HTMLSourceElement": "off",
        "HTMLSpanElement": "off",
        "HTMLStyleElement": "off",
        "HTMLTableCaptionElement": "off",
        "HTMLTableCellElement": "off",
        "HTMLTableColElement": "off",
        "HTMLTableElement": "off",
        "HTMLTableRowElement": "off",
        "HTMLTableSectionElement": "off",
        "HTMLTemplateElement": "off",
        "HTMLTextAreaElement": "off",
        "HTMLTimeElement": "off",
        "HTMLTitleElement": "off",
        "HTMLTrackElement": "off",
        "HTMLUListElement": "off",
        "HTMLVideoElement": "off",
        "HTMLMediaElement": "off",
        "HTMLElement": "off",
        "MathMLElement": "off",
        "SVGElement": "off",
        "Document": "off",
        "Element": "off",
        "Window": "off",
        "URLSearchParams": "off",
        "ReadableStream": "off",
        "WritableStream": "off",
        "XMLHttpRequest": "off"
    }
}
